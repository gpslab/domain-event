{
    "name": "gpslab/domain-event",
    "license": "MIT",
    "type": "library",
    "homepage": "https://github.com/gpslab/domain-event",
    "description": "Tools to create the domain layer of your DDD application",
    "autoload": {
        "psr-4": {
            "GpsLab\\Domain\\Event\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GpsLab\\Domain\\Event\\Tests\\": "tests/"
        },
        "classmap": [
            "tests/Event/Acme_Demo_PurchaseOrderCreated.php"
        ]
    },
    "require": {
        "php": ">=5.5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.*",
        "scrutinizer/ocular": "1.3.*",
        "satooshi/php-coveralls": "^1.0",
        "symfony/dependency-injection": "~2.3|~3.0",
        "symfony/serializer": "~2.3|~3.0",
        "predis/predis": "~1.0"
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    }
}
