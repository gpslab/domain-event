{
    "name": "gpslab/domain-event",
    "license": "MIT",
    "type": "library",
    "homepage": "https://github.com/gpslab/domain-event",
    "description": "Tools to create the domain layer of your DDD application",
    "keywords": [
        "infrastructure",
        "ddd",
        "domain-event",
        "predis",
        "amqp",
        "symfony"
    ],
    "autoload": {
        "psr-4": {
            "GpsLab\\Domain\\Event\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GpsLab\\Domain\\Event\\Tests\\": "tests/"
        },
        "classmap": [
            "tests/Fixture/Acme_Demo_PurchaseOrderCreated.php"
        ]
    },
    "require": {
        "php": ">=5.5.0"
    },
    "require-dev": {
        "psr/container": "~1.0",
        "psr/log": "~1.0",
        "symfony/dependency-injection": "~2.3|~3.0|~4.0",
        "symfony/serializer": "~2.3|~3.0|~4.0",
        "predis/predis": "~1.0|~1.1",
        "php-amqplib/php-amqplib": "^2.9",
        "phpunit/phpunit": "^4.8.36",
        "scrutinizer/ocular": "~1.5",
        "php-coveralls/php-coveralls": "^2.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "2.2-dev"
        }
    }
}
